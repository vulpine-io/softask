{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "api-type-list",
  "title": "API Type Listing",
  "description": "Listing of types used by this API",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "id": {
        "title": "Type ID",
        "type": "string"
      },
      "link": {
        "title": "Type Def URL",
        "description": "Link to the definition of this type.",
        "type": "string"
      }
    },
    "required": [
      "id",
      "link"
    ],
    "additionalProperties": false
  },
  "minItems": 1
}